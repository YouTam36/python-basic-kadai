        "def price_tax(price, tax_rate=0.1):\n",
        "    tax_amount = price * tax_rate\n",
        "    total_price = price + tax_amount\n",
        "    return total_price\n",
        "\n",
        "total_price = price_tax(100)\n",
        "print(f\"{total_price}円\")\n"
